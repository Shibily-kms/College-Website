<section class="our-teacher">
    <div class="main-title title-image">
        <h1>THE SKSSF</h1>
        <div class="icons {{#if adminHere}} edit-boarder {{/if}}">
        {{#if adminHere}} <a class="admin-edit edit-icon-content" data-title="Edit" target="_blank" href="/admin/skssf/skssf-links"><i
        class="bi bi-pencil-fill"></i></a> {{/if}}
          {{#if SkssfLinks.Discord}}<a href="{{SkssfLinks.Discord}}" target="_blank" data-title="Discord"><i class="bi bi-discord"></i></a> {{/if}}
          {{#if SkssfLinks.Email}}<a href="mailto:{{SkssfLinks.Email}}" target="_blank" data-title="Email"><i class="bi bi-envelope-fill"></i></a> {{/if}}
          {{#if SkssfLinks.Facebook}}<a href="{{SkssfLinks.Facebook}}" target="_blank" data-title="Facebook"><i class="bi bi-facebook"></i></a> {{/if}}
          {{#if SkssfLinks.Github}}<a href="{{SkssfLinks.Github}}" target="_blank" data-title="Github"><i class="bi bi-github"></i></a> {{/if}}
          {{#if SkssfLinks.Website}}<a href="{{SkssfLinks.Website}}" target="_blank" data-title="Website"><i class="bi bi-globe"></i></a> {{/if}}
          {{#if SkssfLinks.Instagram}}<a href="{{SkssfLinks.Instagram}}" target="_blank" data-title="Instagram"><i class="bi bi-instagram"></i></a> {{/if}}
          {{#if SkssfLinks.Linkedin}}<a href="{{SkssfLinks.Linkedin}}" target="_blank" data-title="Linkedin"><i class="bi bi-linkedin"></i></a> {{/if}}
          {{#if SkssfLinks.Messenger}}<a href="{{SkssfLinks.Messenger}}" target="_blank" data-title="Messenger"><i class="bi bi-messenger"></i></a> {{/if}}
          {{#if SkssfLinks.Mobile}}<a href="{{SkssfLinks.Mobile}}" target="_blank" data-title="Mobile"><i class="bi bi-telephone-fill"></i></a> {{/if}}
          {{#if SkssfLinks.Signal}}<a href="{{SkssfLinks.Signal}}" target="_blank" data-title="Signal"><i class="bi bi-signal"></i></a> {{/if}}
          {{#if SkssfLinks.Telegram}}<a href="{{SkssfLinks.Telegram}}" target="_blank" data-title="Telegram"><i class="bi bi-telegram"></i></a> {{/if}}
          {{#if SkssfLinks.Tiktok}}<a href="{{SkssfLinks.Tiktok}}" target="_blank" data-title="Tiktok"><i class="bi bi-tiktok"></i></a> {{/if}}
          {{#if SkssfLinks.Twitter}}<a href="{{SkssfLinks.Twitter}}" target="_blank" data-title="Twitter"><i class="bi bi-twitter"></i></a> {{/if}}
          {{#if SkssfLinks.Whatsapp}}<a href="{{SkssfLinks.Whatsapp}}" target="_blank" data-title="Whatsapp"><i class="bi bi-whatsapp"></i></a> {{/if}}
          {{#if SkssfLinks.Youtube}}<a href="{{SkssfLinks.Youtube}}" target="_blank" data-title="Youtube"><i class="bi bi-youtube"></i></a> {{/if}}
        </div>
    </div>
</section>
<section class="container" style="margin-top: -90px;">
    {{!-- NSA content --}}
    <div class="content-div {{#if adminHere}} edit-boarder {{/if}}">
        {{#if adminHere}} <a class="admin-edit edit-icon-content" data-title="Edit" href="/admin/skssf/skssf-about"
            target="_blank"><i class="bi bi-pencil-fill"></i></a> {{/if}}
       
        <p class="perag">{{SkssfPara.Para1}}</p>
        <p class="perag">{{SkssfPara.Para2}}</p>
        <p class="perag">{{SkssfPara.Para3}}</p>
        <p class="perag">{{SkssfPara.Para4}}</p>
        <p class="perag">{{SkssfPara.Para5}}</p>
        
        <div class="pera-buttens">
            {{#if SkssfPara.Btn1.Name}}
            <a href="{{SkssfPara.Btn1.Link}}" {{#if SkssfPara.Btn1.New}} target="_blank" {{/if}}><button class="btn-lg">{{SkssfPara.Btn1.Name}} {{#if SkssfPara.Btn1.New}} <i class="bi bi-arrow-up-right"></i> {{/if}} </button></a>
            {{/if}}
            {{#if SkssfPara.Btn2.Name}}
            <a href="{{SkssfPara.Btn2.Link}}" {{#if SkssfPara.Btn2.New}} target="_blank" {{/if}}><button class="btn-lg">{{SkssfPara.Btn2.Name}} {{#if SkssfPara.Btn2.New}} <i class="bi bi-arrow-up-right"></i> {{/if}} </button></a>
            {{/if}}
            
        </div>
        
    </div>

    <div style="margin-top: 0;" class="four-profile {{#if adminHere}} edit-boarder {{/if}}">
        {{#if adminHere}} <a class="admin-edit edit-icon-content" href="/admin/skssf/skssf-members" data-title="Edit"
            target="_blank"><i class="bi bi-pencil-fill"></i></a> {{/if}}

        {{!-- 25% --}}

        
        {{#each SkssfPro}}
        <div id="foru-profile" class="profile ">
            <div>
                <div class="image">
                    <img src="/images/profiles/{{this.Id}}.jpg" alt="">
                </div>
                <div class="content">
                    <h4>{{this.FullName}}</h4>
                    <p>- {{this.Position}} -</p>
                    <hr>
                    <p>{{this.Address}}</p>
                    <p>{{this.Mobile}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>




</section>


{{!-- Color change --}}

<script>

    let pre = document.getElementById('Header').innerHTML
    
    const first = pre.split(' ').shift();
    document.getElementById("Header").innerHTML = pre.replace(first, `<span class ="first-word">` + first + `</span>`);

</script>